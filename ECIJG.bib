	volume = {10572},
	doi = {10.1117/12.2285960},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85038430294&doi=10.1117%2f12.2285960&partnerID=40&md5=ed8f78b41f513f6f568bdda1a91865f2},
