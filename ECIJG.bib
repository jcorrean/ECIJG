@CONFERENCE{Gomez-Vargas2023,
	author = {Gomez-Vargas, Daniel and Pino, Angie and Garzon, Andrea and Roberti, Flavio and Carelli, Ricardo and Munera, Marcela and Cifuentes, Carlos A.},
	title = {Neuro-Rehabilitation Therapy with T-FLEX Ankle Exoskeleton and Serious Games: A Case Study},
	year = {2023},
	journal = {IEEE International Conference on Rehabilitation Robotics},
	doi = {10.1109/ICORR58425.2023.10304765},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85176465293&doi=10.1109%2fICORR58425.2023.10304765&partnerID=40&md5=18700d714d20c03c52c10cdf1249da9a},
	affiliations = {Institute of Automatics, National University of San Juan, Argentina; Colombian School of Engineering Julio Garavito, Colombia; University of the West of England, Bristol Robotics Laboratory, United Kingdom},
	abstract = {Acquired Brain Injury (ABI) causes permanent disabilities, such as foot drop. This condition affects the gait pattern, increasing the metabolic cost and risk of falling. Robotics with serious games has shown promising results in the gait rehabilitation context. This paper aims to analyze the effects of using the T-FLEX exoskeleton with (1) Automated Therapy (AT) and (2) Serious Game Therapy (SGT) in two ABI patients. Each participant completed six assisted sessions for each strategy. Results showed that AT increases the user-robot interaction torque by 10% for the first patient and 70% for the second patient, and SGT decreases by 5% for both patients. This way, SGT required the patient to generate torque to execute the ankle movement, while AT did the opposite, resulting in greater device assistance. In the functional assessment, SGT induced variations greater than 50% for the paretic ankle and knee's range of motion (ROM), indicating a potential for motor recovery. Thus, SGT led to improved ankle control and increased gait speed compared to AT. These findings suggest that SGT may be an effective rehabilitation strategy for ABI-related foot drop patients. © 2023 IEEE.},
	keywords = {Ankle; Ankle Joint; Exoskeleton Device; Gait; Humans; Peroneal Neuropathies; Robotics; Drops; Exoskeleton (Robotics); Patient rehabilitation; Patient treatment; Acquired brain injuries; Case-studies; Condition; Foot-drop; Gait pattern; Gait rehabilitation; Metabolic cost; Neurorehabilitation []; Rehabilitation therapy; Robot interactions; ankle; gait; human; peroneal neuropathy; robotics; Serious games},
	correspondence_address = {C.A. Cifuentes; University of the West of England, Bristol Robotics Laboratory, United Kingdom; email: carlos.cifuentes@uwe.ac.uk},
	publisher = {IEEE Computer Society},
