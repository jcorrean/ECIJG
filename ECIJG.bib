	doi = {10.1109/ANDESCON61840.2024.10755842},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85211955747&doi=10.1109%2fANDESCON61840.2024.10755842&partnerID=40&md5=36c7a9fb646eeb17df33391551238254},
