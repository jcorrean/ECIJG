@ARTICLE{Maldonado-Mejía2023,
	author = {Maldonado-Mejía, Juan C. and Múnera, Marcela and Diaz, Camilo A. R. and Wurdemann, Helge and Moazen, Mehran and Pontes, Maria José and Vieira Segatto, Marcelo Eduardo and Monteiro, Maxwell E. and Cifuentes, Carlos A.},
	title = {A fabric-based soft hand exoskeleton for assistance: the ExHand Exoskeleton},
	year = {2023},
	journal = {Frontiers in Neurorobotics},
